{
  "version": 2,
  "name": "tetrashop-frontend",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "outputDirectory": "public",
  "github": {
    "enabled": true,
    "autoJobCancelation": true
  }
}
